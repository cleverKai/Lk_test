package com.zuoye.cn;
public class Complex {
	int spacecount = 0;
    int notecount = 0;
    int codecount = 0;
    boolean state = false;
    String c ;
    while((c=fis.readLine())!=null) {
           if(c.contains("/*")) {   //多行注释开始标记
               notecount++;
               state = true;
           }
           else if(state) {
               notecount++;
               if(c.contains("*/")) {  //多行注释结束标记
               state = false;}
           }
           else if(c.contains("//")) {  //单行注释标记
               notecount++;
           }
           else if(c.trim().length()>1) {  //判定为代码行条件
               codecount++;
           }
           else {spacecount++;}
       }
   }

}
